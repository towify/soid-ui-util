    "eslint": "~8.29.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-prettier": "~4.2.1",
    "husky": "~8.0.2",
    "latest-version": "~7.0.0",
    "lint-staged": "~13.1.0",
    "prettier": "~2.8.1",
    "ts-loader": "~9.4.2",
    "typescript": "~4.9.4"
  },
  "version": "0.0.157"
