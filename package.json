  "devDependencies": {
    "@babel/core": "~7.20.5",
    "@babel/plugin-transform-runtime": "~7.19.6",
    "@babel/preset-env": "~7.20.2",
    "@babel/runtime-corejs3": "~7.20.6",
    "@types/node": "~18.11.12",
    "@typescript-eslint/eslint-plugin": "~5.46.0",
    "@typescript-eslint/eslint-plugin-tslint": "~5.46.0",
